
  <h6>贈るシチュエーションに合わせて検索ができます！</h6>
<%= form_with url: search_path, method: :get, local:true do |f| %>
  <%= f.collection_check_boxes :scene_ids, Situation.all, :id, :situation,include_hidden: false %>
  <%= f.select :option, ["おきもち","1"] %>
  <%= f.text_field :search %>

  <%= f.submit '検索' %>
<% end %>